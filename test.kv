<Normal>
	name: 'Normal'
	BoxLayout:
		canvas.before:
			Rectangle:
				pos: self.pos
				size: self.size
				source: 'background.jpg'
		Label:
			text: root.labelText
			font_size: 15
			color: 1,0,0,1
		Button:
			text: 'Return To Main Screen'
			color: 1,0,0,1
			background_normal: 'otherbackgroud.jpg'
			on_release: root.manager.current = 'Home'
