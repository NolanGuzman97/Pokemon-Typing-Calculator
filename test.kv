
<PokeButton@Button>:
	markup: False
	text_size: self.size
	size_hint: .5, 1.0
	border: (4,4,4,4)
	
<HomeScreen>
	name: 'Home'
	GridLayout:
		size_hint: 1.0, 1.0
		size: root.size
		rows: 3
		cols: 6
		canvas.before:
			Rectangle:
				pos: self.pos
				size: self.size
				source: 'pokeball.png'
		
		PokeButton:
			text: "Normal"
			color: 0,0,1,1
			background_normal: 'snorlax.gif'
			on_release: root.manager.current= 'Normal'
		PokeButton:
			text: "Fighting"
			color: 0,0,1,1
			background_normal: 'machamp.gif'
			on_release: root.manager.current = 'Fighting'
		PokeButton:
			text: "Flying"
			color: 0,0,1,1
			background_normal: 'staraptor.gif'
			on_release: root.manager.current = 'Flying'
		PokeButton:
			text: "Poison"
			color: 0,0,1,1
			background_normal: 'nidoking.gif'
			on_release: root.manager.current = 'Poison'
		PokeButton:
			text: "Ground"
			color: 0,0,1,1
			background_normal: 'groudon.gif'
			on_release: root.manager.current = 'Ground'
		PokeButton:
			text: "Rock"
			color: 0,0,1,1
			background_normal: 'onix.gif'
			on_release: root.manager.current = 'Rock'
		PokeButton:
			text: "Bug"
			color: 0,0,1,1
			background_normal: 'caterpie.gif'
			on_release: root.manager.current = 'Bug'
		PokeButton:
			text: "Ghost"
			color: 0,0,1,1
			background_normal: 'gengar.gif'
			on_release: root.manager.current = 'Ghost'
		PokeButton:
			text: "Steel"
			color: 0,0,1,1
			background_normal: 'steelix.gif'
			on_release: root.manager.current = 'Steel'
		PokeButton:
			text: "Fire"
			color: 0,0,1,1
			background_normal: 'charizard.gif'
			on_release: root.manager.current = 'Fire'
		PokeButton:
			text: "Water"
			color: 0,0,1,1
			background_normal: 'blastoise.gif'
			on_release: root.manager.current = 'Water'
		PokeButton:
			text: "Grass"
			color: 0,0,1,1
			background_normal: 'venusaur.gif'
			on_release: root.manager.current = 'Grass'
		PokeButton: 
			text: "Electric"
			color: 0,0,1,1
			background_normal: 'pikachu.gif'
			on_release: root.manager.current = 'Electric'
		PokeButton:
			text: "Psychic"
			color: 0,0,1,1
			background_normal: 'alakazam.zip'
			on_release: root.manager.current = 'Psychic'
		PokeButton:
			text: "Ice"
			color: 0,0,1,1
			background_normal: 'glaceon.gif'
			on_release: root.manager.current = 'Ice'
		PokeButton:
			text: "Dragon"
